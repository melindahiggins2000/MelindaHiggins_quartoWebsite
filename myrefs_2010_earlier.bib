@inproceedings{haran_handheld_2004,
	title = {Handheld thermal imaging for law enforcement and counterdrug applications},
	volume = {5403},
	doi = {10.1117/12.541705},
	abstract = {Although the potential benefits of infrared imaging systems in law enforcement applications have been apparent for many years, budget and technology constraints have prevented their widespread deployment. Recent technology improvements and cost reductions, however, have made the routine use of handheld thermal imagers practical for the law enforcement community. This effort involved both an analysis of operational and technical requirements associated with law enforcement use as well as a comprehensive review of commercially available handheld infrared imaging systems. The use of handheld infrared systems in the counterdrug environment is also addressed, including the results of an analysis of proven applications, a review of training requirements, and a summary of legal issues associated with infrared surveillance. Results generated by the NVTHERM infrared sensor model are also shown for one handheld imager to provide representative information regarding low-cost thermal imager performance.},
