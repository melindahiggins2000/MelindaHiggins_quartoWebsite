@article{marin_feeding_2014,
	title = {Feeding preterm infants during red blood cell transfusion is associated with a decline in postprandial mesenteric oxygenation.},
	volume = {165},
	url = {https://www.ncbi.nlm.nih.gov/pubmed/24948351},
	doi = {10.1016/j.jpeds.2014.05.009},
	abstract = {OBJECTIVE: To evaluate the mesenteric tissue oxygenation response in preterm infants fed and not fed during red blood cell (RBC) transfusions. STUDY DESIGN: Prospective, observational comparison of mesenteric oxygenation using near-infrared spectroscopy in preterm infants ({\textless}33 weeks' at birth) who were fed or not fed during RBC transfusion. Tissue oxygenation means were examined up to 48 hours after each transfusion event. RESULTS: Mean mesenteric regional oxygen saturation (rSO2) slopes during RBC transfusion of fed (n = 9) vs not fed (n = 8) infants ranged from -0.23 to +0.23 (mean 0.04) with no differences between groups (P = .480). However, following transfusions, postprandial mesenteric oxygenation means significantly declined in infants fed during transfusion compared with infants not fed during transfusion (P {\textless} .001). Infants fed during RBC transfusion had a mean 2.16 point decrease in rSO2 mesenteric oxygenation with each sequential feeding post-transfusion, whereas infants not fed during RBC transfusion increased their rSO2 postprandial mesenteric oxygenation by a mean of 2.09 points. CONCLUSIONS: Mesenteric tissue oxygenation during RBC transfusion is not influenced by feeding status. However, infants fed during RBC transfusion had, for the next 15 hours, decreasing postprandial mesenteric tissue oxygenation patterns compared with infants not fed during RBC transfusion. Feeding during RBC transfusions may increase the risk for mesenteric ischemia and the development of transfusion-related necrotizing enterocolitis in preterm infants.},
	language = {eng},
	number = {3},
	journal = {J Pediatr},
	author = {Marin, T and Josephson, CD and Kosmetatos, N and Higgins, M and Moore, JE},
	month = sep,
	year = {2014},
	note = {Publisher: United States},
	keywords = {Prospective Studies},
	pages = {464--71.e1},
}

